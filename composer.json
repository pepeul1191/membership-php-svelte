{
	"name": "pepe/flightphp2",
	"description": "FlightPHP Boilerplate",
	"authors": [
		{
			"name": "Pepe Valdivia",
			"email": "jvaldivia@softweb.pe"
		}
	],
	"require": {
		"php": "^7.2 || ^8.0",
    "bcosca/fatfree-core": "^3.7",
		"j4mie/paris":"^1.5.6",
		"j4mie/idiorm": "^1.5.3",
		"phpmailer/phpmailer": "^6.1",
		"vlucas/phpdotenv": "^4.1"
	},
	"scripts": {
		"start": "php -S localhost:8080",
		"dump": "composer dump-autoload -o"
